<Window x:Class="RFClassicClicker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RFClassicClicker"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="705,177,0,0" VerticalAlignment="Top" Click="button_Click"/>
        <Button x:Name="sendKeyBtn" Content="F1 в первое окно каждые 5 секунд" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="sendKeyBtn_Click" Tag="E"/>
        <Button x:Name="sendKeyBtn2" Content="F1 во второе окно каждые 5 секунд" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Click="sendKeyBtn2_Click"/>
        <Button x:Name="button1" Content="Button" HorizontalAlignment="Left" Margin="290,187,0,0" VerticalAlignment="Top" Click="button1_Click"/>

    </Grid>
</Window>
